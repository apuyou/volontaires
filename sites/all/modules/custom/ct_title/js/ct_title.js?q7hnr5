(function ($, Drupal, window, document, undefined) {
  Drupal.behaviors.ct_title = {
    attach: function (context, settings) {
      var data = settings.ct_title;

      // pour les vues data.title sera vide
      if (data.title) {
        $('.pane-page-title > h1').html(data.title);
      }
    }
  };
})(jQuery, Drupal, this, this.document);